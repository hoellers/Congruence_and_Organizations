// Start of JavaScript
// Define the Dimensions

var attRaw = ["Other members are", "Leader is",
"Organization's headquarters located in", "Organization is",
"Funding mostly comes from",
"Aiming to increase voter registration"];

var att = ["Other members are", "Leader is",
"Organization's headquarters located in", "Organization is", "Funding mostly comes from",
 "Aiming to increase voter registration"];

var attributes = ["","","","","",""];

for (i=0; i<attRaw.length;i++ )
{
var rand1 = Math.floor(Math.random()*((attRaw.length-i)-0));
attributes[i] = att[rand1];
att.splice(rand1,1);
}


// Create Variables for Traits associated with each dimension.
var mem_raw = ["mainly students","students and non-students","mainly non-students"];
var lead_raw = ["a student","<b>not</b> a student"];
var hq_raw = ["Chapel Hill, NC","Raleigh, NC", "Richmond, VA", "Washington, DC"];
var type_raw = ["<b>not</b> a chapter of a national organization ", "a chapter of a national organization"]
var fund_raw = ["donations from members and community","donations from national partners"];
var goal_raw = ["on campus", "in the town of Chapel Hill", "throughout North Carolina"];


//Use math.random to randomly select attributes for each dimension.
//For instance, "gender_raw[Math.floor(Math.random()*2)];" randomly pulls
//either the first (male) or second (female) element of gender_raw.
var mem_a__1__1 = mem_raw[Math.floor(Math.random()*mem_raw.length)];
var lead_a__1__1 = lead_raw[Math.floor(Math.random()*lead_raw.length)];
var hq_a__1__1 = hq_raw[Math.floor(Math.random()*hq_raw.length)];
var type_a__1__1 = type_raw[Math.floor(Math.random()*type_raw.length)];
var fund_a__1__1 = fund_raw[Math.floor(Math.random()*fund_raw.length)];
var goal_a__1__1 = goal_raw[Math.floor(Math.random()*goal_raw.length)];

//Use math.random to randomly select attributes for each dimension.
var mem_b__1__1 = mem_raw[Math.floor(Math.random()*mem_raw.length)];
var lead_b__1__1 = lead_raw[Math.floor(Math.random()*lead_raw.length)];
var hq_b__1__1 = hq_raw[Math.floor(Math.random()*hq_raw.length)];
var type_b__1__1 = type_raw[Math.floor(Math.random()*type_raw.length)];
var fund_b__1__1 = fund_raw[Math.floor(Math.random()*fund_raw.length)];
var goal_b__1__1 = goal_raw[Math.floor(Math.random()*goal_raw.length)];


// Create array for first candidate's attributes
// Create array for second candidate's attributes
var att_a_copy__1__1 = [1,2,3,4,5,6];
var att_b_copy__1__1 = [1,2,3,4,5,6];

// Take indexOf the Dimension variable. This is the order of the Dimension variable
var mem_index__1__1 = attributes.indexOf("Other members are");
var lead_index__1__1 = attributes.indexOf("Leader is");
var hq_index__1__1 = attributes.indexOf("Organization's headquarters located in");
var type_index__1__1 = attributes.indexOf("Organization is");
var fund_index__1__1 = attributes.indexOf("Funding mostly comes from");
var goal_index__1__1 = attributes.indexOf("Aiming to increase voter registration");


// Use IndexOf Dimension and replace with appropriate trait for Candidate A
// For instance, if "Gender" is the 3rd element in the Dimension variable,
//this will place Male/Female as the 3rd element of the first candidate's attributes.
att_a_copy__1__1[mem_index__1__1] = mem_a__1__1;
att_a_copy__1__1[lead_index__1__1] = lead_a__1__1;
att_a_copy__1__1[hq_index__1__1] = hq_a__1__1;
att_a_copy__1__1[type_index__1__1] = type_a__1__1;
att_a_copy__1__1[fund_index__1__1] = fund_a__1__1;
att_a_copy__1__1[goal_index__1__1] = goal_a__1__1;

// Do the same for the second candidate
att_b_copy__1__1[mem_index__1__1] = mem_b__1__1;
att_b_copy__1__1[lead_index__1__1] = lead_b__1__1;
att_b_copy__1__1[hq_index__1__1] = hq_b__1__1;
att_b_copy__1__1[type_index__1__1] = type_b__1__1;
att_b_copy__1__1[fund_index__1__1] = fund_b__1__1;
att_b_copy__1__1[goal_index__1__1] = goal_b__1__1;


// Create list of variables in the for loop
att_list__1__1 = ["att1__1__1","att2__1__1","att3__1__1","att4__1__1","att5__1__1","att6__1__1"];
a_list__1__1 = ["a1__1__1","a2__1__1","a3__1__1","a4__1__1","a5__1__1","a6__1__1"];
b_list__1__1 = ["b1__1__1","b2__1__1","b3__1__1","b4__1__1","b5__1__1","b6__1__1"];

// For elements 1 to 8 in the Dimensions, First Candidate and Second Candidate variables.
// The first line assigns Dimensions to each "id" tag in the first column of the HTML table
// The second line assigns first candidate traits to each "id" tag in the second
//column of the HTML table
// The third line assigns second candidate traits to each "id" tag in the third
//column of the HTML table
for(i=0;i<6;i++){
    document.getElementById(att_list__1__1[i]).innerHTML= attributes[i];
    document.getElementById(a_list__1__1[i]).innerHTML= att_a_copy__1__1[i];
    document.getElementById(b_list__1__1[i]).innerHTML= att_b_copy__1__1[i];    }

//Set embedded data for values produced
{
	Qualtrics.SurveyEngine.setEmbeddedData('mem1', mem_a__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData('lead1', lead_a__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData( 'hq1', hq_a__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData('type1', type_a__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData( 'fund1', fund_a__1__1 );
	Qualtrics.SurveyEngine.setEmbeddedData('goal1', goal_a__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData( 'mem2', mem_b__1__1 );
	Qualtrics.SurveyEngine.setEmbeddedData('lead2', lead_b__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData( 'hq2', hq_b__1__1 );
	Qualtrics.SurveyEngine.setEmbeddedData('type2', type_b__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData( 'fund2',fund_b__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData('goal2', goal_b__1__1);
	Qualtrics.SurveyEngine.setEmbeddedData('attributes', attributes)
};


//for recording attributes in Qualtrics
Qualtrics.SurveyEngine.addOnload(function()
{
/*Place Your Javascript Below This Line*/
	this.setChoiceValue(1, "${e://Field/mem1}");
  this.setChoiceValue(2,"${e://Field/lead1}");
	this.setChoiceValue(3,"${e://Field/hq1}");
	this.setChoiceValue(4,"${e://Field/type1}");
	this.setChoiceValue(5,"${e://Field/fund1}");
	this.setChoiceValue(6,"${e://Field/goal1}");
	this.setChoiceValue(7, "${e://Field/mem2}");
  this.setChoiceValue(8,"${e://Field/lead2}");
	this.setChoiceValue(9,"${e://Field/hq2}");
	this.setChoiceValue(10,"${e://Field/type2}");
	this.setChoiceValue(11,"${e://Field/fund2}");
	this.setChoiceValue(12,"${e://Field/goal2}");
	this.setChoiceValue(13,"${e://Field/attributes}");
	/*this.getQuestionContainer().hide();*/
});
